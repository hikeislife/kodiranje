<div id="header-top">
  <svg id="logo" preserveAspectRatio="none" style="height: 70px;width: 100vw;" xmlns="https://www.w3.org/2000/svg">
    <desc>logo sajta</desc>
    <g id="bcg" style="stroke: #b1b1b1;fill: none;stroke-width: 1;stroke-dasharray: 1, 10;">
      <path d="M5 5 5000 5" />
      <path d="M5 15 5000 15" />
      <path d="M5 25 5000 25" />
      <path d="M5 35 5000 35" />
      <path d="M5 45 5000 45" />
      <path d="M5 55 5000 55" />
      <path d="M5 65 5000 65" />
      <path d="M5 75 5000 75" />
      <path d="M5 85 5000 85" />
    </g>

    <a href="/" rel="start chapter" aria-label="homepage">
      <g id="logo-text">
        <text x="40" y="48"
          style="font-size: 48px; font-family:'Roboto Mono', monospace; font-display: swap;">kodiranje_</text>
      </g>
    </a>

    <g id="rounded-square" style="fill:none;stroke:#1A1A1A;stroke-width:2;width:35;height:35;">
      <defs>
        <filter id="f1">
          <feOffset result="offOut" in="SourceGraphic" dx="1" dy="1" />
          <feGaussianBlur result="blurOut" in="offOut" stdDeviation="2" />
          <feBlend in="SourceGraphic" in2="blurOut" mode="normal" />
        </filter>
      </defs>
      <rect x="37" y="17" rx="5" ry="5" width="35" height="35" filter="url(#f1)" />
    </g>

    <g id="fake-selection" style="fill: #e5e5e5;">
      <circle cx="32.5" cy="12.5" r="3" />
      <circle cx="75.5" cy="12.5" r="3" />
      <circle cx="32.5" cy="55.25" r="3" />
      <circle cx="75.5" cy="55.25" r="3" />
      <g style="stroke:#e5e5e5;stroke-width:0.5;">
        <line class="logo-sel" x1="32.5" y1="15.75" x2="32.5" y2="52" />
        <line class="logo-sel" x1="75.5" y1="15.75" x2="75.5" y2="52" />
        <line class="logo-sel" x1="35.75" y1="12.5" x2="72.15" y2="12.5" />
        <line class="logo-sel" x1="35.75" y1="55.5" x2="72.15" y2="55.5" />
      </g>
    </g>

    <g id="cog" display="none">
      <image x="10" y="10" width="30px" height="30px" href="cog.webp">
        <animateTransform attributeName="transform" attributeType="XML" type="rotate" from="0 25 25" to="360 25 25"
          dur="10s" repeatCount="indefinite" />
      </image>
    </g>
  </svg>
</div>

<!-- absolutely do not change any of this, it will render the image inaccessible  -->
<div id="mobile-cog">
  <img
    src="data:image/png;base64,"
    class="logo-cog" alt="mob-menu" width="40" height="40" />
</div>